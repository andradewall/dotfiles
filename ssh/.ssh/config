# Common defaults
Host *
  ServerAliveInterval 60
  IdentitiesOnly yes
  AddKeysToAgent yes

# Personal GitHub
Host wllc.github.com
  HostName github.com
  User git
  IdentityFile ~/.ssh/wllc_github_ed25519

# Work GitHub
Host safe.github.com
  HostName github.com
  User git
